[
    {
        "match": ".well-known",
        "ignore": false
    },
    {
        "match": ".well-known/ii-alternative-origins",
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Content-Type": "application/json"
        },
        "ignore": false
    },
    {
        "match": "**/*.webp",
        "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
        },
        "security_policy": "standard"
    },
    {
        "match": "**/*.png",
        "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
        },
        "security_policy": "standard"
    },
    {
        "match": "**/*.svg",
        "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
        },
        "security_policy": "standard"
    },
    {
        "match": "**/*.mp4",
        "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
        },
        "security_policy": "standard"
    },
    {
        "match": "**/*.webm",
        "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
        },
        "security_policy": "standard"
    },
    {
        "match": "**/*.woff",
        "headers": {
            "Cache-Control": "public, max-age=31536000, immutable"
        },
        "security_policy": "standard"
    },
    {
        "match": "**/*.woff2",
